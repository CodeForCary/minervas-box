{
  "name": "artProject",
  "version": "0.0.0",
  "description": "Maintains and displays city curated artwork.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "xo",
    "develop": "http-server -c-1 -o"
  },
  "keywords": [
    "code-for-america"
  ],
  "author": "Juan Orozco",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.2.3",
    "del": "^1.1.1",
    "documentjs": "^0.3.0-pre.1",
    "gulp": "^3.8.11",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.3",
    "gulp-util": "^3.0.4",
    "require-dir": "^0.1.0",
    "xo": "^0.16.0"
  },
  "dependencies": {
    "bit-data-suite": "git+https://github.com/bitovi-components/bit-data-suite.git#master",
    "bootstrap": "^3.3.6",
    "can": "^2.3.24",
    "jquery": "^2.2.0",
    "leaflet": "^0.7.7",
    "lodash": "^4.13.1",
    "promise": "^7.1.1",
    "steal": "^0.16.21",
    "steal-tools": "^0.16.4"
  },
  "system": {
    "ignoreBrowser": true,
    "paths": {
      "bootstrap": "./node_modules/bootstrap/dist/js/bootstrap.js",
      "bootstrap/*": "./node_modules/bootstrap/js/*.js",
      "components/*": "./src/components/*.js",
      "models/*": "./src/models/*.js",
      "pages/*": "./src/pages/*.js",
      "utils/*": "./src/utils/*.js"
    },
    "npmAlgorithm": "flat",
    "npmIgnore": [
      "devDependencies"
    ]
  },
  "xo": {
    "envs": [
      "node"
    ],
    "space": 4,
    "rules": {
      "no-lonely-if": 0,
      "no-else-return": 0,
      "spaced-comment": 0,
      "no-warning-comments": 0,
      "import/no-extraneous-dependencies": 0
    },
    "globals": [
      "steal",
      "chai"
    ],
    "ignore": [
      "node_modules/**",
      "dist/**",
      "_dist/**",
      "docs/**"
    ],
    "overrides": [
      {
        "files": "src/**",
        "esnext": true
      }
    ]
  }
}
