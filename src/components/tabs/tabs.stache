<ul class="nav nav-tabs" role="tablist">
    {{#tabs}}
    <li role="presentation" class="{{#active}}active{{/active}}">
        <button type="button"
            class="btn-tab"
            aria-controls="{{tabId}}"
            role="tab"
            data-toggle="tab"
            can-click="toggleTab">{{tabTitle}}</button>
    </li>
    {{/tabs}}
</ul>

<div class="tab-content">
    <content></content>
</div>
