<ul class="nav nav-tabs" role="tablist">
    {{#tabs}}
    <li role="presentation" class="{{#isActive}}active{{/isActive}}">
        <button type="button"
            class="btn-tab"
            aria-controls="{{tabId}}"
            role="tab"
            data-toggle="tab"
            can-click="toggleTab">{{itemTitle}}</button>
    </li>
    {{/tabs}}
</ul>

<div class="tab-content">
    <content></content>
</div>
